
@{
    ViewBag.Title = "Exitpage";
    Layout = null;
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, shrink-to-fit=no">
    <title>Pagina de error  </title>
    @*<link rel="icon" type="image/x-icon" href="assets/img/favicon.ico" />*@
    <link rel="icon" type="image/x-icon" href="~/Content/Template/assets/img/favicon.ico" />
    <!-- BEGIN GLOBAL MANDATORY STYLES -->
    <link href="https://fonts.googleapis.com/css?family=Nunito:400,600,700" rel="stylesheet">
    <link href="~/Content/Template/bootstrap/css/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/Template/assets/css/plugins.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/Template/assets/css/pages/error/style-500.css" rel="stylesheet" type="text/css" />
    <!-- END GLOBAL MANDATORY STYLES -->

    <!--ANALITICA-->
    <!-- Global site tag (gtag.js) - Google Analytics -->
    <script async src="https://www.googletagmanager.com/gtag/js?id=G-CPKBCXVS5S"></script>
    <script>
        window.dataLayer = window.dataLayer || [];
        function gtag() { dataLayer.push(arguments); }
        gtag('js', new Date());
        gtag('config', 'G-CPKBCXVS5S');
    </script>
    <!--END ANALITICA-->

</head>
<body class="error500 text-center">

    <div class="container-fluid">
        <div class="row">
            <div class="col-md-4 mr-auto mt-5 text-md-left text-center">
                <a href="index.html" class="ml-md-5">
                    @*<img alt="image-500" src="~/Content/Template/assets/img/favicon.ico" class="theme-logo">*@
                </a>
            </div>
        </div>
    </div>

    <div class="container-fluid error-content">
        <div class="">
            @*<h1 class="error-number">@ViewBag.Code</h1>*@
            <p class="mini-text">@ViewBag.Message</p>
            <p class="error-text">Sera redirigido en 10 segundos</p>
            <div class="progress br-30 progress-md">
                <div id="barra" class="progress-bar bg-danger progress-bar-striped progress-bar-animated" role="progressbar" style="width: 10%" aria-valuenow="100" aria-valuemin="0" aria-valuemax="100">0%</div>
            </div>
            <!-- <a href="index.html" class="btn btn-secondary mt-5">Go Back</a> -->
        </div>
    </div>

    <!-- BEGIN GLOBAL MANDATORY SCRIPTS -->
    <script src="~/Content/Template/assets/js/libs/jquery-3.1.1.min.js"></script>
    <script src="~/Content/Template/bootstrap/js/popper.min.js"></script>
    <script src="~/Content/Template/bootstrap/js/bootstrap.min.js"></script>
    <script src="~/Scripts/Pages/ExitPage.js?id=532"></script>
    <!-- END GLOBAL MANDATORY SCRIPTS -->
</body>
</html>